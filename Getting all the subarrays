CODE:
#include <iostream>
#include <vector>
#include <algorithm>
#include <climits>
#include <string>
#include <cmath>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n;
    cin >> n;

    int arr[n];
    for (int i=0; i<n; i++) {
        cin >> arr[i];
    }

    for (int st=0; st<n; st++) {
        for (int end=st; end<n; end++) {
            for (int i=st; i<end; i++) {
                cout << arr[i];
            }
            cout << " ";
        }
        cout << endl;
    }

    return 0;
}


OUTPUT: 
5
1 2 3 4 5
1 12 123 1234 
2 23 234
3 34
4
