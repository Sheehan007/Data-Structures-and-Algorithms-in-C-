#include <iostream>
#include <vector>
#include <algorithm>
#include <climits>
#include <string>
#include <cmath>

using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int n=5;
    int arr[5] = {1,2,3,4,5};

    int maxsum = INT_MIN;

    for (int st=0; st<n; st++) {
        int currsum=0;
        for (int end=st; end<n; end++) {
            currsum += arr[end];
            maxsum = max(currsum, maxsum);
        }
    }

    cout << "max subarray sum: " << maxsum << endl;

    return 0;
}


OUTPUT: 
max subarray sum: 15
