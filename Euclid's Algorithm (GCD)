#include <iostream>
#include<bits/stdc++.h>
#include<vector>
#include<algorithm>
#include<climits>
#include<string>
#include<cmath>
#include<unordered_set>
#include<utility>
#include<bitset>

using namespace std;

int gcd(int a, int b) {
    while (a>0 && b>0) {
        if(a > b) {
            a = a % b;
        }
        else {
            b = b % a;
        }
    }
    

if (a == 0) return b;
else return a;

}

int main() {
    cout << gcd(12,6) << endl;
    return 0;   



OUTPUT: 
6
}
