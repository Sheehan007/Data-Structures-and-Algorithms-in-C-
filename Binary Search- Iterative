#include <iostream>
#include <vector>
#include <algorithm>
#include <climits>
#include <string>
#include <cmath>

using namespace std;

int binary_search(vector<int> arr, int tar) {
    int st=0, end = arr.size()-1;

    while (st <= end) {
        int mid = (st+end)/2;

        if (tar < arr[mid]) {
            st = mid-1;
        }
        else if (tar > arr[mid]) {
            st = mid+1;
        }
        else {
            return mid;
        }
    }

    return -1;
}

int main() {
    vector<int> arr1 = {-1,0,3,4,5,9,12};
    int tar1 = 0;
    cout << binary_search(arr1, tar1) << endl;

}

OUTPUT: 
1


OUTPU
